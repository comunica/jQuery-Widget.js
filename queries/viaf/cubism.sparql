# 5. Works by Cubists
# Datasources: https://fragments.dbpedia.org/*/en https://data.linkeddatafragments.org/viaf
SELECT ?name ?work ?title {
  ?artist dbpedia-owl:movement [ rdfs:label "Cubism"@en ];
              foaf:name ?name.
  ?work schema:author [ schema:sameAs ?artist ];
              schema:name ?title.
  FILTER (!REGEX(?name, ","))
}
